{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    //"locale": "es",
    "lib": ["es2023"],
    "module": "node16",
    "moduleResolution": "node16",
    "pretty": true,
    "sourceMap": true,
    "target": "es2022",
    "outDir": "./dist",
    "baseUrl": "./",
    "esModuleInterop": true
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}